<?xml version="1.0" encoding="utf-8" ?>
<!--
配置编辑器支持的控件和可修改的属性
-->
<suprot>
    <!-- DisplayObject -->
    <Image>
        <name/>
        <x/>
        <y/>
        <texture/>
        <width/>
        <height/>
        <alpha/>
        <visible/>
        <touchable/>
        <scaleX/>
        <scaleY/>
    </Image>
    <Scale3Image>
        <name/>
        <x/>
        <y/>
        <texture/>
        <width/>
        <height/>
        <alpha/>
        <visible/>
        <touchable/>
    </Scale3Image>
    <Scale9Image>
        <name/>
        <x/>
        <y/>
        <texture/>
        <width/>
        <height/>
        <alpha/>
        <visible/>
        <touchable/>
    </Scale9Image>
    <TiledImage>
        <name/>
        <x/>
        <y/>
        <texture/>
        <width/>
        <height/>
        <alpha/>
        <visible/>
        <touchable/>
    </TiledImage>
    <ImageLoader>
        <name/>
        <x/>
        <y/>
        <source/>
        <errorTexture/>
        <loadingTexture/>
        <maintainAspectRatio/>
        <width/>
        <height/>
        <alpha/>
        <visible/>
        <touchable/>
    </ImageLoader>
    <!-- Controls -->
    <Label>
        <name/>
        <x/>
        <y/>
        <text/>
        <color/>
        <fontSize/>
        <fontWeight/>
        <fontName/>
        <width/>
        <height/>
        <alpha/>
        <visible/>
        <touchable/>
        <minWidth/>
        <minHeight/>
        <maxWidth/>
        <maxHeight/>
    </Label>
    <TextInput>
        <name/>
        <x/>
        <y/>
        <text/>
        <color/>
        <fontSize/>
        <fontWeight/>
        <fontName/>
        <prompt/>
        <promptColor/>
        <displayAsPassword/>
        <isEditable/>
        <backgroundSkin/>
        <defaultIcon/>
        <gap/>
        <padding/>
        <paddingLeft/>
        <paddingRight/>
        <paddingTop/>
        <paddingBottom/>
        <width/>
        <height/>
        <alpha/>
        <visible/>
        <touchable/>
        <minWidth/>
        <minHeight/>
        <maxWidth/>
        <maxHeight/>        
    </TextInput>
    <Button>
        <name/>
        <x/>
        <y/>
        <label/>
        <width/>
        <height/>
        <defaultSkin/>
        <downSkin/>
        <disabledSkin/>
        <defaultIcon/>
        <alpha/>
        <visible/>
        <touchable/>
        <minWidth/>
        <minHeight/>
        <maxWidth/>
        <maxHeight/>
        <fontSize/>
        <color/>
        <fontWeight/>
        <fontName/>
        <gap/>
        <iconPosition/>
        <iconOffsetX/>
        <iconOffsetY/>
        <labelOffsetX/>
        <labelOffsetY/>
        <padding/>
        <paddingLeft/>
        <paddingRight/>
        <paddingTop/>
        <paddingBottom/>
    </Button>
    <ToggleButton>
        <name/>
        <x/>
        <y/>
        <label/>
        <isSelected/>
        <width/>
        <height/>
        <defaultSkin/>
        <defaultSelectedSkin/>
        <downSkin/>
        <disabledSkin/>
        <defaultIcon/>
        <defaultSelectedIcon/>
        <alpha/>
        <visible/>
        <touchable/>
        <minWidth/>
        <minHeight/>
        <maxWidth/>
        <maxHeight/>
        <fontSize/>
        <color/>
        <fontWeight/>
        <fontName/>
        <gap/>
        <iconPosition/>
        <iconOffsetX/>
        <iconOffsetY/>
        <labelOffsetX/>
        <labelOffsetY/>
        <padding/>
        <paddingLeft/>
        <paddingRight/>
        <paddingTop/>
        <paddingBottom/>
    </ToggleButton>
    <!-- Container -->
    <LayoutGroup>
        <name/>
        <x/>
        <y/>
        <width/>
        <height/>
        <alpha/>
        <visible/>
        <touchable/>
        <minWidth/>
        <minHeight/>
        <maxWidth/>
        <maxHeight/>
        <!-- layout properites -->
        <layout/>
        <gap/>
        <padding/>
        <paddingLeft/>
        <paddingRight/>
        <paddingTop/>
        <horizontalAlign/>
        <verticalAlign/>
    </LayoutGroup>
    <ProgressBar>
        <name/>
        <x/>
        <y/>
        <value/>
        <width/>
        <height/>
        <backgroundSkin/>
        <fillSkin/>
        <alpha/>
        <visible/>
        <touchable/>
        <minWidth/>
        <minHeight/>
        <maxWidth/>
        <maxHeight/>
    </ProgressBar>
    <List>
        <name/>
        <x/>
        <y/>
        <width/>
        <height/>
        <backgroundSkin/>
        <alpha/>
        <visible/>
        <touchable/>
        <minWidth/>
        <minHeight/>
        <maxWidth/>
        <maxHeight/>
        <!-- layout properites -->
        <layout/>
        <gap/>
        <padding/>
        <paddingLeft/>
        <paddingRight/>
        <paddingTop/>
        <horizontalAlign/>
        <verticalAlign/>
        <!-- virtual -->
        <itemRenderer/>
        <dataNum/><!-- only for editor -->
    </List>
</suprot>